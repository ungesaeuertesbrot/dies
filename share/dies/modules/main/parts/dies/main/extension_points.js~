const IFace = imports.malus.iface;

function test_gui (obj)
{
	return IFace.implements_interface (obj, {
				window: "!undefined",
				toolkit: "string",
				present: "function",
				hide: "function",
				run: "function",
				quit: "function"
			});
}

function test_storage (obj)
{
	return true;
}

function test_data_mgr (obj)
{
	return IFace.implements_interface (obj, {
				id: "!undefined",
				type: "string",
				multi: "boolean",
				storage: "!undefined",
				create_collection: "function"
			});
}

